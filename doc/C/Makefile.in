all: 

grip.html: grip/grip.html
	-cp grip/grip.html .

grip/grip.html: $(SGML_FILES)
	-db2html -c /etc/sgml/catalog grip.sgml
	-cp *.png grip

dist-hook:
	mkdir $(distdir)/grip
	-cp grip/*.html grip/*.css grip/*.png $(distdir)/grip
	-cp grip.html $(distdir)

install-data-local: grip.html
	$(mkinstalldirs) $(DESTDIR)$(grip_helpdir)/images
	-for file in $(srcdir)/grip/*.html $(srcdir)/grip/*.css $(srcdir)/grip/*.png; do \
	basefile=`basename $$file`; \
	$(INSTALL_DATA) $(srcdir)/$$file  $(DESTDIR)$(grip_helpdir)/$$basefile; \
	done

grip.ps: grip.sgml
	-db2ps $<

grip.rtf: grip.sgml       
	-db2rtf $<
